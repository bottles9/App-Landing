<!DOCTYPE html>
<html>
<head>

    <meta charset="UTF-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="A complete landing page solution for any business">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="~/img/favicon/favicons.ico">
    <title>@ViewBag.Title</title>
    <link href="~/Content/Style.css" rel="stylesheet" type="text/css" />



</head>
<body id="top">
    <header class="header header-shrink header-inverse fixed-top">
        <div class="container">
            <nav class="navbar navbar-expand-lg px-md-0">
                <a class="navbar-brand" href="index.html">
                    <span class="logo-default">
                        <h2 class="logo-brand-color">Devdap</h2>
                    </span>
                    <span class="logo-inverse">
                        <h2 class="logo-brand-color-inverse">Devdap</h2>
                    </span>
                </a>
                <button class="navbar-toggler p-0" data-toggle="collapse" data-target="#navbarNav">
                    <div class="hamburger hamburger--spin js-hamburger">
                        <div class="hamburger-box">
                            <div class="hamburger-inner"></div>
                        </div>
                    </div>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active">
                            <a class="nav-link" href="#" data-scrollto="top">HOME</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#" data-scrollto="about">about</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#" data-scrollto="features">FEATURES</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#" data-scrollto="screenshot">SCREENSHOTS</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#" data-scrollto="reviews">REVIEWS</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#" data-scrollto="faq">FAQ</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#" data-scrollto="blog">Blog</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link btn btn btn-rounded btn-green u-w-160 ml-lg-4 text-capitalize" href="#" data-scrollto="download">Download Now</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
    </header>

    //Render The Body
    @RenderBody()

    <footer style="background-image: url('../../img/app/footer.jpg');background-size:cover;background-position:center center;">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="bg-black box-shadow-v1 rounded p-5">
                        <h2 class="h1 text-center text-white">
                            Subscribe to Our Newsletter
                        </h2>
                        <form class="form-inline justify-content-center my-5 subsribe-rounded">
                            <div class="form-group">
                                <input type="email" class="form-control px-4 u-rounded-50 u-h-55" placeholder="Enter your email address">
                            </div>
                            <button type="submit" class="btn btn-rounded btn-green u-h-55">
                                Subscribe Now
                            </button>
                        </form>
                    </div>
                </div>
                <div class="col-12 text-center u-py-60">
                    <h2 class="logo-brand-footer">Devdap</h2>
                    <ul class="list-inline social social-rounded mt-4">
                        <li class="list-inline-item">
                            <a href="#"><i class="fa fa-facebook"></i></a>
                        </li>
                        <li class="list-inline-item">
                            <a href="#"><i class="fa fa-twitter"></i></a>
                        </li>
                        <li class="list-inline-item">
                            <a href="#"><i class="fa fa-google-plus"></i></a>
                        </li>
                        <li class="list-inline-item">
                            <a href="#"><i class="fa fa-linkedin"></i></a>
                        </li>
                        <li class="list-inline-item">
                            <a href="#"><i class="fa fa-dribbble"></i></a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="row align-items-center" style="border-top:1px solid #c3f4df;color:#c3f4df">
                <div class="col-lg-6 py-4 text-center text-lg-left">
                    <span>© Copyright 2018 - Created by <a target="_blank" href="http://devdap.com">devdap</a></span>
                </div>
                <div class="col-lg-6 py-4">
                    <ul class="list-inline footer-nav u-ff-dosis text-center text-lg-right">
                        <li class="list-inline-item">
                            <a class="text-uppercase u-fw-600 p-2" href="#">HOME</a>
                        </li>
                        <li class="list-inline-item">
                            <a class="text-uppercase u-fw-600 p-2" href="#">elements</a>
                        </li>
                        <li class="list-inline-item">
                            <a class="text-uppercase u-fw-600 p-2" href="#">about</a>
                        </li>
                        <li class="list-inline-item">
                            <a class="text-uppercase u-fw-600 p-2" href="#">BLOG</a>
                        </li>
                        <li class="list-inline-item">
                            <a class="text-uppercase u-fw-600 p-2" href="#">CONTACT</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
   
    <script src="~/Scripts/bundle.js"></script>
    <script src="~/Scripts/devdap.js"></script>
</body>
</html>